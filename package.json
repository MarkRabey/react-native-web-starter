{
  "name": "react-native-web-starter",
  "version": "0.0.3",
  "description": "A very basic, somewhat opinionated, template for React Native Web",
  "scripts": {
    "postversion": "yarn run commit-version",
    "commit-version": "git add . && git commit -m \"chore(release): publish `node -p 'require(\"./package.json\").version'`\"",
    "test": "exit 0"
  },
  "files": [
    "template",
    "template.config.js"
  ],
  "keywords": [
    "react-native",
    "react-native-web",
    "typescript",
    "template",
    "starter"
  ],
  "author": "Mark Rabey <mark@markrabey.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MarkRabey/react-native-web-starter/issues"
  },
  "homepage": "https://github.com/MarkRabey/react-native-web-starter#readme"
}
